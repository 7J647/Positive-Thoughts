  "bugs": {
    "url": "https://github.com/Andrew0502/Positive-Thoughts/issues"
  },
  "homepage": "https://github.com/Andrew0502/Positive-Thoughts#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.11",
    "path": "^0.12.7",
    "react-router-dom": "^5.2.0",
    "twilio": "^3.51.0"
  },
